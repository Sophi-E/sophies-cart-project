is.handleAddItem.bind(this);\n    this.handleRemoveItem = this.handleRemoveItem.bind(this);\n    this.handleEmptyCart = this.handleEmptyCart.bind(this);\n    this.toggleModal = this.toggleModal.bind(this);\n  }\n\n  handleItemQuantityChange(event:any){\n    // This will handle the quantity buttons updating. In order\n    //  to work, we need two things from the clicked button: a\n    //  data-id key and the value (1 or -1).\n    const clickedEl = event.target;\n    const qty = Number(clickedEl.value);\n    const id = Number(clickedEl.dataset.id);\n\n    this.setState(prevState=>({\n      ...prevState,\n      cart: prevState.cart.map((item:any)=>item.id===id ? {...item, quantity: item.quantity+qty} : item )\n    }))\n  }\n\n  handleAddItem(event:any){\n    const clickedEl = event.target;\n    const id = Number(clickedEl.dataset.id);\n    let cart;\n    this.setState(prevState=>{\n      if(prevState.cart.find((item:any)=>item.id===id)){\n        cart = prevState.cart.map((cartItem:any)=>cartItem.id===id?{...cartItem, quantity: cartItem.quantity+1}:cartItem)\n      } else {\n        cart = [...prevState.cart, {id, quantity:1}]\n      }\n      return {\n        ...prevState,\n        cart\n      }\n    })\n  }\n\n  handleRemoveItem(event:any){\n    const clickedEl = event.target;\n    const id = Number(clickedEl.dataset.id);\n\n    this.setState(prevState=>({\n      ...prevState,\n      cart: prevState.cart.filter((item:any)=>item.id!==id)\n    }))\n  }\n\n  handleEmptyCart(){\n    this.setState(prevState=>({\n      ...prevState,\n      cart: []\n    }))\n  }\n\n  toggleModal(){\n    this.setState((prevState:any)=>({...prevState, modalOpen:!prevState.modalOpen}) )\n  }\n\n  componentDidMount(){\n    fetch('https://fakestoreapi.com/products')\n        .then(res=>res.json())\n        .then(json=>this.setState(prevState=>({...prevState, store: [...json]})))\n\n  }\n\n  render(){\n    return (\n      <>\n        <header className=\"flex space-between\">\n          <h1 className=\"w-1/2 text-4xl font-black text-indigo-600\">Shop O Stuff</h1>\n          <a href=\"#\" onClick={this.toggleModal}>\n            <span className='w-1/4 text-md text-indigo-300'>Your cart contains {this.state.cart.length} items.</span>\n          </a>\n        </header>\n\n        <StoreListing onAddToCart={this.handleAddItem} items={this.state.store} />\n\n        <Modal isOpen={this.state.modalOpen}\n               onRequestClose={this.toggleModal}\n               contentLabel=\"My cart\"\n               >\n          <div>My cart!</div>\n          <a href=\"#\" onClick={this.toggleModal}>Close cart</a>\n        </Modal>\n      </>\n\n    );\n    }\n}\n\nexport default App;\n","/home/toby/Projects/sophies-cart/src/index.tsx",[],["55","56"],"/home/toby/Projects/sophies-cart/src/components/quantity-widget/quantity-widget.tsx",["57"],"/home/toby/Projects/sophies-cart/src/components/quantity-widget/index.ts",[],"/home/toby/Projects/sophies-cart/src/components/line-item/line-item.tsx",[],"/home/toby/Projects/sophies-cart/src/components/line-item/index.ts",[],"/home/toby/Projects/sophies-cart/src/components/store-listing/index.js",[],"/home/toby/Projects/sophies-cart/src/components/store-listing/store-listing.tsx",["58","59"],"/home/toby/Projects/sophies-cart/src/components/cart-listing/index.js",[],"/home/toby/Projects/sophies-cart/src/components/cart-listing/cart-listing.tsx",["60","61"],{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":5,"column":8,"nodeType":"68","messageId":"69","endLine":5,"endColumn":16},{"ruleId":"66","severity":1,"message":"70","line":8,"column":11,"nodeType":"68","messageId":"69","endLine":8,"endColumn":19},{"ruleId":"71","severity":1,"message":"72","line":109,"column":11,"nodeType":"73","endLine":109,"endColumn":50},{"ruleId":"71","severity":1,"message":"72","line":121,"column":11,"nodeType":"73","endLine":121,"endColumn":45},{"ruleId":"62","replacedBy":"74"},{"ruleId":"6